[user]
	name = Tifan Dwi Avianto
	email = 33323177+tifandotme@users.noreply.github.com
	signingkey = ~/.ssh/id_ed25519.pub
[color]
	ui = auto
[init]
	defaultbranch = master
[commit]
	gpgsign = true
[gpg]
	format = ssh
[push]
    default = current
    autoSetupRemote = true
    followTags = true
[pull]
	rebase = true
[rebase]
	autoStash = true
	missingCommitsCheck = warn
[remote "origin"]
	prune = true
[url "git@github.com:"]
	insteadOf = https://github.com/
