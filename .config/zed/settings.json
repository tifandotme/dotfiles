{
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "base_keymap": "VSCode",
  "theme": {
    "dark": "Neo Gruber Darker",
    "light": "Gruvbox Light Hard",
    "mode": "system"
  },
  "restore_on_startup": "last_workspace",
  "format_on_save": "off",
  "file_finder": {
    "file_icons": false
  },
  "collaboration_panel": {
    "button": false
  },
  "project_panel": {
    "default_width": 300,
    "indent_guides": {
      "show": "never"
    },
    "dock": "right",
    "auto_fold_dirs": false,
    "indent_size": 20,
    "folder_icons": false,
    "file_icons": false,
    "hide_root": true
  },
  "outline_panel": {
    "default_width": 300,
    "indent_guides": {
      "show": "never"
    },
    "dock": "right",
    "auto_fold_dirs": false
  },
  "git_panel": {
    "button": false
  },
  "terminal": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "agent": {
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "default_width": 600,
    "default_model": {
      "provider": "copilot_chat",
      "model": "grok-code-fast-1"
    },
    "play_sound_when_agent_done": true,
    "inline_assistant_model": {
      "provider": "copilot_chat",
      "model": "grok-code-fast-1"
    },
    "profiles": {
      "ask": {
        "context_servers": {
          "github": {
            "tools": {
              "get_code_scanning_alert": true,
              "get_commit": true,
              "get_dependabot_alert": true,
              "get_discussion": true,
              "get_discussion_comments": true,
              "get_file_contents": true,
              "get_global_security_advisory": true,
              "get_issue": true,
              "get_issue_comments": true,
              "get_job_logs": true,
              "get_latest_release": true,
              "get_me": true,
              "get_notification_details": true,
              "get_pull_request": true,
              "get_pull_request_comments": true,
              "get_pull_request_diff": true,
              "get_pull_request_files": true,
              "get_pull_request_reviews": true,
              "get_pull_request_status": true,
              "get_release_by_tag": true,
              "get_secret_scanning_alert": true,
              "get_tag": true,
              "get_team_members": true,
              "get_teams": true,
              "get_workflow_run": true,
              "get_workflow_run_logs": true,
              "get_workflow_run_usage": true,
              "list_branches": true,
              "list_code_scanning_alerts": true,
              "list_commits": true,
              "list_dependabot_alerts": true,
              "list_discussion_categories": true,
              "list_discussions": true,
              "list_gists": true,
              "list_global_security_advisories": true,
              "list_issue_types": true,
              "list_issues": true,
              "list_notifications": true,
              "list_org_repository_security_advisories": true,
              "list_pull_requests": true,
              "list_releases": true,
              "list_repository_security_advisories": true,
              "list_secret_scanning_alerts": true,
              "list_sub_issues": true,
              "list_tags": true,
              "list_workflow_jobs": true,
              "list_workflow_run_artifacts": true,
              "list_workflow_runs": true,
              "list_workflows": true,
              "search_code": true,
              "search_issues": true,
              "search_orgs": true,
              "search_pull_requests": true,
              "search_repositories": true,
              "search_users": true
            }
          },
          "server-sequential-thinking": {
            "tools": {
              "sequentialthinking": true
            }
          },
          "exa": {
            "tools": {
              "crawling_exa": true,
              "web_search_exa": true
            }
          },
          "context7-mcp": {
            "tools": {
              "resolve-library-id": true,
              "get-library-docs": true
            }
          }
        },
        "name": "Ask",
        "tools": {
          "open": true,
          "contents": true,
          "diagnostics": true,
          "list_directory": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "thinking": true
        }
      },
      "write": {
        "context_servers": {
          "exa": {
            "tools": {
              "linkedin_search_exa": false,
              "deep_researcher_start": false,
              "deep_researcher_check": false,
              "company_research_exa": false
            }
          }
        },
        "name": "Write",
        "tools": {
          "project_notifications": true,
          "open": true,
          "create_directory": true,
          "terminal": true,
          "batch_tool": true,
          "code_actions": true,
          "code_symbols": true,
          "contents": true,
          "copy_path": true,
          "create_file": true,
          "delete_path": true,
          "diagnostics": true,
          "edit_file": true,
          "fetch": false,
          "list_directory": true,
          "move_path": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "rename": true,
          "symbol_info": true,
          "thinking": true
        },
        "enable_all_context_servers": true
      }
    }
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "context_servers": {},
  "buffer_font_family": "CommitMono Nerd Font",
  "buffer_font_size": 12.5,
  "agent_font_size": 17,
  "buffer_line_height": "standard",
  "use_autoclose": false,
  "current_line_highlight": "all",
  "relative_line_numbers": true,
  "cursor_blink": false,
  "search_wrap": false,
  "wrap_guides": [80, 100],
  "soft_wrap": "editor_width",
  "inlay_hints": {
    "enabled": false,
    "show_type_hints": false,
    "show_parameter_hints": true,
    "show_other_hints": false,
    "show_background": false,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "vim_mode": true,
  "vim": {
    "highlight_on_yank_duration": 300,
    "toggle_relative_line_numbers": true,
    "use_system_clipboard": "on_yank"
  },
  "buffer_font_features": {
    "calt": false
  },
  "diagnostics": {
    "inline": {
      "enabled": false
    }
  },
  "lsp": {
    "biome": {
      "settings": {
        "require_config_file": true
      }
    },
    "tailwindcss-language-server": {
      "settings": {
        "classAttributes": ["class", "className", "ngClass", "styles"]
      }
    }
  },
  "languages": {
    "XML": {
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--parser", "html", "--stdin-filepath", "{buffer_path}"]
        }
      }
    },
    "Shell Script": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "shfmt",
          "arguments": ["--filename", "{buffer_path}", "--indent", "2"]
        }
      }
    },
    "Kdl": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "kdlfmt",
          "arguments": ["format", "--stdin"]
        }
      }
    },
    "Nu": {
      "tab_size": 4,
      "formatter": {
        "external": {
          "command": "/Users/tifan/.config/nushell/scripts/format.nu"
        }
      },
      "format_on_save": "on"
    },
    "CSS": {
      "tab_size": 2
    },
    "Lua": {
      "tab_size": 2
    },
    "Markdown": {
      "show_edit_predictions": true
    }
  }
}
